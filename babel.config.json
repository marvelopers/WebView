{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    [
      "@emotion",
      {
        "labelFormat": "[dirname]_[filename]--[local]",
        "cssPropOptimization": true
      }
    ],
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": false,
        "preprocess": false
      }
    ]
  ]
}
